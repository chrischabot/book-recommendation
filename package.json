{
  "name": "book-recommender",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "docker:up": "docker compose -f docker/docker-compose.yml up -d",
    "docker:up:cache": "docker compose -f docker/docker-compose.yml --profile cache up -d",
    "docker:down": "docker compose -f docker/docker-compose.yml down",
    "docker:logs": "docker compose -f docker/docker-compose.yml logs -f",
    "dev": "pnpm docker:up && next dev",
    "build": "next build",
    "start": "pnpm docker:up && next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "migrate": "pnpm docker:up && tsx db/migrate.ts",
    "download:ol": "tsx scripts/download-openlibrary.ts",
    "ingest:ol": "pnpm docker:up && tsx scripts/ingest-openlibrary.ts",
    "enrich:gb": "pnpm docker:up && tsx scripts/enrich-googlebooks.ts",
    "enrich:authors": "pnpm docker:up && tsx scripts/enrich-authors.ts",
    "link:authors": "pnpm docker:up && tsx scripts/link-work-authors-bulk.ts",
    "cooccur:build": "pnpm docker:up && tsx scripts/build-cooccurrence.ts",
    "community:build": "pnpm docker:up && tsx scripts/build-communities.ts",
    "enrich:descriptions": "pnpm docker:up && tsx scripts/enrich-descriptions.ts",
    "import:goodreads": "pnpm docker:up && tsx scripts/import-goodreads.ts",
    "import:kindle": "pnpm docker:up && tsx scripts/import-kindle.ts",
    "aggregate:kindle": "pnpm docker:up && tsx scripts/aggregate-kindle-reading.ts",
    "kindle:enrich": "pnpm docker:up && tsx scripts/kindle-enrich.ts",
    "kindle:dedupe": "pnpm docker:up && tsx scripts/kindle-dedupe.ts",
    "dedupe:cross-source": "pnpm docker:up && tsx scripts/dedupe-cross-source.ts",
    "kindle:fix-unknowns": "pnpm docker:up && tsx scripts/kindle-fix-unknowns.ts",
    "fetch:ol-isbns": "pnpm docker:up && tsx scripts/fetch-ol-unknown-isbns.ts",
    "enrich:unknown-gb": "pnpm docker:up && tsx scripts/enrich-unknown-googlebooks.ts",
    "embed:user-events": "pnpm docker:up && tsx scripts/embed-user-events.ts",
    "features:embed": "pnpm docker:up && tsx scripts/build-embeddings.ts",
    "embeddings:backup": "pnpm docker:up && tsx scripts/backup-embeddings.ts backup",
    "embeddings:restore": "pnpm docker:up && tsx scripts/backup-embeddings.ts restore",
    "refresh:views": "pnpm docker:up && tsx scripts/refresh-views.ts",
    "profile:build": "pnpm docker:up && tsx scripts/build-user-profile.ts",
    "graph:build": "pnpm docker:up && tsx scripts/build-graph.ts",
    "refresh:all": "pnpm docker:up && tsx scripts/refresh-all.ts",
    "update": "pnpm docker:up && tsx scripts/update.ts",
    "test": "vitest run"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "bottleneck": "^2.19.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "csv-parse": "^5.6.0",
    "dotenv": "^17.2.3",
    "fast-fuzzy": "^1.12.0",
    "ioredis": "^5.8.2",
    "js-yaml": "^4.1.1",
    "lucide-react": "^0.556.0",
    "motion": "^12.23.25",
    "next": "16.0.7",
    "openai": "^4.104.0",
    "p-limit": "^7.2.0",
    "pg": "^8.16.3",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-markdown": "^10.1.0",
    "remark-breaks": "^4.0.0",
    "remark-gfm": "^4.0.1",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "undici": "^6.22.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.1",
    "@next/eslint-plugin-next": "^16.0.7",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.19.1",
    "@types/pg": "^8.15.6",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "autoprefixer": "^10.4.22",
    "eslint": "^9.39.1",
    "eslint-config-next": "16.0.7",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "playwright": "^1.57.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.1",
    "vitest": "^2.1.9"
  }
}
